<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.myspace.bbs.Service.impl.BbsMapper">

	<!-- <select id="login" parameterType="" resultType="">
	
	
	</select> -->
	
	<select id="selectBbsDate" resultType="Map">
		
		SELECT NOW();
	
	</select>
	
	<select id="getEmpName" parameterType="java.util.HashMap"
		resultType="java.util.HashMap">
		SELECT * FROM T_WORKGROUP WHERE COUNTRY = #{COUNTRY}
	</select>
	
	<select id="selectTestList" resultType="Map">
		SELECT *FROM T_WORKGROUP;
	</select>
	
	<insert id="testingInsert" parameterType="com.myspace.bbs.Service.BbsDTO"> 
		INSERT INTO T_WORKGROUP 
		 VALUES ( #{emp_id}, #{emp_no}, #{emp_name}, #{country}, #{dept} ) 
	
	</insert>

</mapper> 

